<%- include('../partials/header') %>

<h1>üèÜ User Performance Report</h1>

<table>
  <thead>
    <tr>
      <th>Rank</th>
      <th>Name</th>
      <th>Total Projects</th>  <!-- ‡πÄ‡∏û‡∏¥‡πà‡∏° -->
      <th>Total Tasks</th>
      <th>Completed</th>
      <th>Total Time (min)</th>
      <th>Completion %</th>
    </tr>
  </thead>
  <tbody>
    <% users.forEach(user => { %>
      <tr>
        <td><%= user.rank %></td>
        <td><%= user.name %></td>
        <td><%= user.total_projects || 0 %></td> <!-- ‡πÄ‡∏û‡∏¥‡πà‡∏° -->
        <td><%= user.total_tasks || 0 %></td>
        <td><%= user.completed_tasks || 0 %></td>
        <td><%= user.total_time_spent || 0 %></td>
        <td><%= user.completion_rate || 0 %> %</td>
      </tr>
    <% }) %>
  </tbody>
</table>

<%- include('../partials/footer') %>